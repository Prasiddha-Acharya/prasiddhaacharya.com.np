&lt;div class=&quot;quiz-over&quot;&gt;
&lt;div class=&quot;box&quot;&gt;
&lt;p&gt;
      Good Try!&lt;br /&gt;
      You Got &lt;span class=&quot;correct-answers&quot;&gt;&lt;/span&gt; out of &lt;span class=&quot;total-question2&quot;&gt;&lt;/span&gt; answers correct! &lt;br /&gt;
      That&#39;s &lt;span class=&quot;percentage&quot;&gt;&lt;/span&gt;
     &lt;/p&gt;
&lt;button onclick=&quot;tryAgain()&quot; type=&quot;button&quot;&gt;TryAgain&lt;/button&gt;
   &lt;/div&gt;
&lt;/div&gt;
&lt;script src=&quot;script.js&quot;&gt;&lt;/script&gt;
&lt;script&gt;
  
  const options=document.querySelector(&quot;.options&quot;).children;
  const answerTrackerContainer=document.querySelector(&quot;.answers-tracker&quot;);
  const questionNumberSpan=document.querySelector(&quot;.question-num-value&quot;);
  const totalQuestionSpan=document.querySelector(&quot;.total-question&quot;);
  const correctAnswerSpan=document.querySelector(&quot;.correct-answers&quot;);
  const totalQuestionSpan2=document.querySelector(&quot;.total-question2&quot;);
  const percentage=document.querySelector(&quot;.percentage&quot;);
  const question=document.querySelector(&quot;.question&quot;);
  const op1=document.querySelector(&quot;.option1&quot;);
  const op2=document.querySelector(&quot;.option2&quot;);
  const op3=document.querySelector(&quot;.option3&quot;);
  const op4=document.querySelector(&quot;.option4&quot;);
  let questionIndex;
  let index=0;
  let myArray=[];
  let myArr=[];
  let score=0;

  // questions and options and answers

  const questions=[
   {
    q:&#39;In which type of database model each node can have only one parent and multiple child?&#39;,
    options:[&#39;a) Hierarchical&#39;,&#39;b) Network&#39;,&#39;c) Relational&#39;,&#39;d) Multilevel&#39;],
    answer:1
   },
   {
    q:&#39;In which type of transmission media data are transmitted in the form of photons instead of electrons?&#39;,
    options:[&#39;a) Twisted pair&#39;,&#39;b) Co-axial&#39;,&#39;c) Fibre optics&#39;,&#39;d) Infrared&#39;],
    answer:3
   },
   {
    q:&#39;How many layers are there in OSI (Open System Interconnection) model? &#39;,
    options:[&#39;a) 4&#39;,&#39;b) 5&#39;,&#39;c) 6&#39;,&#39;d) 7&#39;],
    answer:4
   },
   {
    q:&#39;Which of the following is not the event object of JavaScript? &#39;,
    options:[&#39;(a) onsubmit&#39;,&#39;b) onclick&#39;,&#39;c) onselect&#39;,&#39;d) onpost &#39;],
    answer:4
   },
   {
    q:&#39;What is the Total memory allocated by union? &#39;,
    options:[&#39;a) Sum of all&#39;,&#39;b) Highest among all&#39;,&#39;c) Lowest among all&#39;,&#39;(d) None of above&#39;],
    answer:2
   }, {
    q:&#39;What is the Total memory allocated by union? &#39;,
    options:[&#39;a) Sum of all&#39;,&#39;b) Highest among all&#39;,&#39;c) Lowest among all&#39;,&#39;(d) None of above&#39;],
    answer:2
   }
   
  ]

  // set questions and options and question number
  totalQuestionSpan.innerHTML=questions.length;
  function load(){
        questionNumberSpan.innerHTML=index+1;
         question.innerHTML=questions[questionIndex].q;     
         op1.innerHTML=questions[questionIndex].options[0];
         op2.innerHTML=questions[questionIndex].options[1];
         op3.innerHTML=questions[questionIndex].options[2];
         op4.innerHTML=questions[questionIndex].options[3];
         index++;
  }
  
  function check(element){
   if(element.id==questions[questionIndex].answer){
    element.classList.add(&quot;correct&quot;);
    updateAnswerTracker(&quot;correct&quot;)
    score++;
    console.log(&quot;score:&quot;+score)
   }
   else{
    element.classList.add(&quot;wrong&quot;);
    updateAnswerTracker(&quot;wrong&quot;)

   }
   disabledOptions()
  }
   
  function disabledOptions(){
     for(let i=0; i&lt;options.length; i++) {
      options[i].classList.add(&quot;disabled&quot;);
      if(options[i].id==questions[questionIndex].answer){
       options[i].classList.add(&quot;correct&quot;);
      }

     }
  }
  
  function enableOptions(){
     for(let i=0; i&lt;options.length; i++) {
      options[i].classList.remove(&quot;disabled&quot;,&quot;correct&quot;,&quot;wrong&quot;);
     }
  }

  function validate(){
      if(!options[0].classList.contains(&quot;disabled&quot;)){
        alert(&quot;Please Select one option&quot;)
      }
      else{
       enableOptions();
       randomQuestion();
      }
  }

  function next(){
    validate();
  }
   
  function randomQuestion(){
   let randomNumber=Math.floor(Math.random()*questions.length);
   let hitDuplicate=0;
       if(index==questions.length){
        quizOver();
       }
       else{
         if(myArray.length&gt;0){
             for(let i=0; i&lt;myArray.length; i++){
               if(myArray[i]==randomNumber){
                  hitDuplicate=1;
                  break;
               }
             }
             if(hitDuplicate==1){
              randomQuestion();
             }
             else{
               questionIndex=randomNumber;   
              load();
              myArr.push(questionIndex);
             }
         }
         if(myArray.length==0){
           questionIndex=randomNumber;   
           load();
           myArr.push(questionIndex);
         }

       myArray.push(randomNumber);
       
      }
  }
      
  function answerTrakcer(){
     for(let i=0; i&lt;questions.length; i++){
      const div=document.createElement(&quot;div&quot;)
         answerTrackerContainer.appendChild(div);
     }
  }

 function updateAnswerTracker(classNam){
   answerTrackerContainer.children[index-1].classList.add(classNam);
 }

 function quizOver(){
    document.querySelector(&quot;.quiz-over&quot;).classList.add(&quot;show&quot;);
    correctAnswerSpan.innerHTML=score;
    totalQuestionSpan2.innerHTML=questions.length;
    percentage.innerHTML=(score/questions.length)*100 + &quot;%&quot;;
 }
 
 function tryAgain(){
     window.location.reload();
 }

 window.onload=function(){
  randomQuestion();
  answerTrakcer();
 
}

&lt;/script&gt;
  &lt;/div&gt;&lt;/div&gt;&lt;hr&gt; &lt;p class=&quot;note wr&quot; &gt; We are currently working on this.&lt;/p&gt;</description><link>https://www.iswori.com.np/2023/05/class-12-computer-mcqs.html</link><author>noreply@blogger.com (Iswori)</author><thr:total>0</thr:total><georss:featurename>Nepal</georss:featurename><georss:point>28.394857 84.124007999999989</georss:point><georss:box>0.084623163821152758 48.967757999999989 56.70509083617884 119.28025799999999</georss:box></item><item><guid isPermaLink="false">tag:blogger.com,1999:blog-2127717090755708464.post-1225875768955062769</guid><pubDate>Sat, 20 May 2023 13:17:00 +0000</pubDate><atom:updated>2023-05-22T06:41:46.853-07:00</atom:updated><category domain="http://www.blogger.com/atom/ns#">B.Sc.CSIT</category><category domain="http://www.blogger.com/atom/ns#">Course</category><category domain="http://www.blogger.com/atom/ns#">CSIT Course details</category><category domain="http://www.blogger.com/atom/ns#">Introduction to BSc CSIT in Nepal</category><category domain="http://www.blogger.com/atom/ns#">Salary of Bsc CSIT</category><category domain="http://www.blogger.com/atom/ns#">Scope of BSc CSIT</category><title>BSc CSIT Course In Nepal: Eligibility, Scope, Fee, Colleges</title><description>&lt;div class=&quot;separator&quot; style=&quot;clear: both;&quot;&gt;&lt;a href=&quot;https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEil-eQrcjWcVHdi3drZkjIu0D31yEG0ARlbCDK0QB5eVK3LzI2WU1z75n5vGWsCBAV1s7Yh5uo4CHkwlhRSmVuSeGRR39mjdg8F1QaVzU4LLzIZJxMhNolN4-sfqYyezJT6Re6eHlfXodlSf1_2Rk140Nmx8bNubaszlsb0QO5cMRtlJAnx8Us1gOxwYg/s1600/b-sc-csit-course-in-nepali.png&quot; style=&quot;display: block; padding: 1em 0px; text-align: center;&quot;&gt;&lt;img alt=&quot;BSc CSIT Course In Nepal: Eligibility, Scope, Fee, Colleges&quot; border=&quot;0&quot; data-original-height=&quot;900&quot; data-original-width=&quot;1600&quot; src=&quot;https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEil-eQrcjWcVHdi3drZkjIu0D31yEG0ARlbCDK0QB5eVK3LzI2WU1z75n5vGWsCBAV1s7Yh5uo4CHkwlhRSmVuSeGRR39mjdg8F1QaVzU4LLzIZJxMhNolN4-sfqYyezJT6Re6eHlfXodlSf1_2Rk140Nmx8bNubaszlsb0QO5cMRtlJAnx8Us1gOxwYg/s16000/b-sc-csit-course-in-nepali.png&quot; title=&quot;BSc CSIT Course In Nepal: Eligibility, Scope, Fee, Colleges&quot; /&gt;&lt;/a&gt;&lt;/div&gt;&lt;hr style=&quot;text-align: left;&quot; /&gt;&lt;h2 style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;color: #0b5394; font-family: inherit; font-size: x-large;&quot;&gt;Bachelor of Science in Information Technology and Computer Science (BSc Csit)&lt;/span&gt;&lt;/h2&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;The &lt;a href=&quot;/search/label/B.Sc.CSIT&quot;&gt;Bachelor of Science in Computer Science and Information Technology (BSc. CSIT)&lt;/a&gt; program at Tribhuvan University is a &lt;b&gt;four-year program&lt;/b&gt; designed to provide students with a &lt;b&gt;complete foundation of information technology and computers&lt;/b&gt;.&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Besides from the foundation and core courses in Computer Science and Information Technology, the program provides a variety of options to fulfill the demand for high-tech application development.&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;The foundation and core courses are designed to fulfill the requirements of the undergraduate academic program, whereas the service courses are intended to meet the demands of rapidly developing computer technology and applications.&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;a name=&#39;more&#39;&gt;&lt;/a&gt;&lt;p&gt;&lt;/p&gt;&lt;h3 style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: x-large;&quot;&gt;&lt;u&gt;Objective of CSIT&lt;/u&gt;&lt;/span&gt;&lt;/h3&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;The main goal of the B.Sc.CSIT program is to provide students with in-depth knowledge and skills in various areas of computer science and information technology, such as design, theory, &lt;a href=&quot;https://www.futurelearn.com/info/courses/programming-101/0/steps/43783&quot; target=&quot;_blank&quot;&gt;programming&lt;/a&gt;, and &lt;a href=&quot;http://quora.com/What-are-the-different-applications-of-computer-systems&quot; target=&quot;_blank&quot;&gt;computer system application&lt;/a&gt;. It is expected that graduates of this program will be well-versed in computer software and hardware systems.&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;/p&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;To provide in-depth understanding of computer theory, design, programming, and application.&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Providing in-depth knowledge and expertise with computer systems.&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;In IT, imparting knowledge of the computer and programming logic environment.Advanced IT application knowledge in various business sectors.&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Developing creative and analytical skills as a foundation for technical problem solving. &lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;/p&gt;&lt;div style=&quot;text-align: left;&quot;&gt;&lt;h3 style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit;&quot;&gt;&lt;u&gt;&lt;span style=&quot;font-size: x-large;&quot;&gt;&lt;!--more--&gt;&lt;/span&gt;&lt;span style=&quot;font-size: x-large;&quot;&gt;Duration&lt;/span&gt;&lt;/u&gt;&lt;/span&gt;&lt;/h3&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;The course is divided into &lt;b&gt;Eight semesters (four academic years ).&lt;/b&gt; Each semester concludes with a separate semester examination.&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;/p&gt;&lt;div class=&quot;separator&quot; style=&quot;clear: both; text-align: center;&quot;&gt;&lt;a href=&quot;https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEjP1tXktjKKKKNOlUdbIUVQ4iny3SVQoh_b-OugdMchH2daJYrkBh5axDPNXU8Jjq3faM7QNQnQaLXXqa__rSxjeCEaoYGwbawaYolsoWzWz3wGHSk8bC6wcBUb43-cDswRNB9LYcgd_QTLLBR8EUCoPc567sM4mpHNKKoAdRGLzdIZKz99Zd0vV9Es/s1920/bsc-csit-course-in-nepal.png&quot; style=&quot;margin-left: 1em; margin-right: 1em;&quot;&gt;&lt;span style=&quot;font-family: inherit;&quot;&gt;&lt;img alt=&quot;BSc CSIT Course In Nepal: Eligibility, Scope, Fee, Colleges&quot; border=&quot;0&quot; data-original-height=&quot;1080&quot; data-original-width=&quot;1920&quot; src=&quot;https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEjP1tXktjKKKKNOlUdbIUVQ4iny3SVQoh_b-OugdMchH2daJYrkBh5axDPNXU8Jjq3faM7QNQnQaLXXqa__rSxjeCEaoYGwbawaYolsoWzWz3wGHSk8bC6wcBUb43-cDswRNB9LYcgd_QTLLBR8EUCoPc567sM4mpHNKKoAdRGLzdIZKz99Zd0vV9Es/s16000/bsc-csit-course-in-nepal.png&quot; title=&quot;BSc CSIT Course In Nepal&quot; /&gt;&lt;/span&gt;&lt;/a&gt;&lt;/div&gt;&lt;p&gt;&lt;/p&gt;&lt;div&gt;&lt;hr /&gt;&lt;h2 style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;color: #0b5394; font-family: inherit; font-size: x-large;&quot;&gt;Introduction to BSc CSIT in Nepal&lt;/span&gt;&lt;/h2&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Bachelor of Science in Computer Science and Information Technology (BSc CSIT) is a four-year undergraduate degree program in Nepal that focuses on providing students with a strong foundation in both computer science and information technology. The program is offered by various institutions in Nepal, both private and government-owned, and is highly sought after by students who are interested in pursuing a career in the field of technology.&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;The &lt;a href=&quot;/search/label/CSIT%20Syllabus&quot;&gt;syllabus of BSc CSIT&lt;/a&gt; in Nepal is designed to provide students with a balanced mix of theoretical concepts and practical implementations, and many institutions also offer internship programs in real-world working environments to give students hands-on experience. The program covers a wide range of subjects, including artificial intelligence, data science, big data, and cybersecurity, and is updated regularly to keep up with the latest developments in the field.&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Additionally, Nepal is home to many IT companies based in countries such as Australia, the US, Singapore, and the UK, which offer diverse job opportunities to IT students with attractive salary packages. BSc CSIT students can also gain skills in coding and technical writing and do freelance work even during their degree program. Overall, studying BSc CSIT in Nepal provides students with a wealth of opportunities and a solid foundation for a successful career in the field of IT.&lt;/span&gt;&lt;/p&gt;&lt;!--more--&gt;&lt;p&gt;&lt;/p&gt;&lt;div&gt;&lt;h3&gt;&lt;span style=&quot;font-family: inherit; font-size: x-large;&quot;&gt;&lt;u&gt;Fee Structure of BSc. CSIT In Nepal:&lt;/u&gt;&lt;/span&gt;&lt;/h3&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;The fee structure for the BSc CSIT program may vary depending on the college or university offering the program. In Nepal, the fees for the BSc CSIT program may range from NPR 50,000 to NPR 300,000 per year, depending on the institution.&lt;/span&gt;&lt;/p&gt;&lt;/div&gt;&lt;p&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;/span&gt;&lt;/p&gt;&lt;div&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;In Nepal, &lt;b&gt;government colleges&lt;/b&gt; typically charge around &lt;b&gt;NPR 3-4 lakh&lt;/b&gt; for the four-year program, while &lt;b&gt;private colleges&lt;/b&gt; in the Kathmandu &lt;b&gt;valley&lt;/b&gt; may charge &lt;b&gt;NPR 9-12 lakh&lt;/b&gt;. &lt;b&gt;Private colleges outside the valley&lt;/b&gt; may charge &lt;b&gt;NPR 6-9 lakh&lt;/b&gt; for the four-year program.&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;!--more--&gt;&lt;p&gt;&lt;/p&gt;&lt;h3&gt;&lt;span style=&quot;font-family: inherit; font-size: x-large;&quot;&gt;&lt;u&gt;Salary After BSc CSIT In Nepal:&lt;/u&gt;&lt;/span&gt;&lt;/h3&gt;
  &lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;The salary of a BSc CSIT graduate in Nepal may depend on various factors, such as the individual&#39;s skills, experience, and job responsibilities. According to some estimates, the average salary of a BSc CSIT graduate in Nepal may be around NPR 20,000 to NPR 40,000 per month.&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;!--more--&gt;&lt;p&gt;&lt;/p&gt;&lt;h3&gt;&lt;span style=&quot;font-family: inherit; font-size: x-large;&quot;&gt;&lt;u&gt;Scope of BSc CSIT:&lt;/u&gt;&lt;/span&gt;&lt;/h3&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;The scope of the BSc CSIT program in Nepal is quite wide, as graduates of this program can pursue careers in various fields, including software development, data analysis, network engineering, and more. There are several government colleges in Nepal that offer the BSc CSIT program, as well as private institutions.&lt;/span&gt;&lt;/p&gt;&lt;/div&gt;&lt;hr /&gt;&lt;h2 style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;color: #0b5394; font-family: inherit; font-size: x-large;&quot;&gt;Why study BSc CSIT in Nepal?&lt;/span&gt;&lt;/h2&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;span&gt;B. Sc. CSIT Course In Nepal is Considered as Highly demand Course. It provides Students with strong foundation in both computer science and information technology&lt;/span&gt;&lt;span&gt;&amp;nbsp;which is increasing in Nepal.&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;CSIT program combines theoretical and practical knowledge includes subjects such as computer systems, application, and programming. It focus on application of basic principles of computer science and IT to solve real-world problems, which makes CSIT program suitable for students who want to make career in IT, either in Nepal as well as in abroad.&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;You are allowed to choose elective course and services course in CSIT program as well as it includes Internship and project works, which provides students with practical experience and the opportunity to apply their knowledge in real world application.&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;There are &lt;b&gt;several reasons&lt;/b&gt; &lt;i&gt;&lt;b&gt;why studying BSc CSIT in Nepal&lt;/b&gt;&lt;/i&gt; can be a great choice for students:&lt;/span&gt;&lt;/p&gt;  &lt;p style=&quot;text-align: left;&quot;&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;/p&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;b&gt;Balanced curriculum:&lt;/b&gt;&amp;nbsp;&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;b&gt;Internship opportunities&lt;/b&gt;&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;b&gt;Job demand&lt;/b&gt;&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;b&gt;Global acceptance&lt;/b&gt;&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;b&gt;Affordability&lt;/b&gt;&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;p&gt;&lt;/p&gt;&lt;p&gt;&lt;/p&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Overall, studying BSc CSIT in Nepal can provide students with the necessary knowledge and skills to succeed in the competitive field of IT, and to pursue a rewarding career in this field.&lt;br /&gt;&lt;/span&gt;&lt;hr /&gt;&lt;h2 style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;color: #0b5394; font-family: inherit; font-size: x-large;&quot;&gt;Eligibility criteria for BSc CSIT in Nepal&lt;/span&gt;&lt;/h2&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;To be eligible for admission to the BSc CSIT program in Nepal, students must meet the following criteria: Have successfully completed 12 years of schooling in the science stream or equivalent from a university, board, or institution recognized by TU.&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;/p&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Have passed the entrance examination conducted by TU with at least 35% marks&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Have completed all application procedures&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Have secured a minimum of a second division ( Final Grade C ) in their +2 or equivalent.&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;p&gt;&lt;/p&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Alternatively, students may also be eligible for admission to the BSc CSIT program if they:&lt;/span&gt;&lt;div&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;/p&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Have passed A level in Science (have studied Physics and Mathematics with 100 marks) and passed at least in D Grade&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;ul style=&quot;text-align: left;&quot;&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Have passed a 3-year Diploma in Engineering program from the Council for Technical Education and Vocational Training (CTEVT) taking both Physics and Mathematics&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/ul&gt;&lt;div&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Source: &lt;a href=&quot;https://www.tuiost.edu.np/storage/notice/bsccsit-entrance079250.pdf&quot; target=&quot;_blank&quot;&gt;BSc CSIT 2079 Notice&lt;/a&gt;&lt;/span&gt;&lt;/div&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;It is important to note that students from both Biology and Mathematics groups of +2 level are eligible to apply for the BSc CSIT program.&lt;/span&gt;&lt;/p&gt;&lt;div&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;b&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Question Pattern:&lt;/span&gt;&lt;/b&gt;&lt;/p&gt;&lt;table id=&quot;simple_table&quot; style=&quot;text-align: center;&quot;&gt;&lt;tbody&gt;&lt;tr&gt;	&lt;th style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;u&gt;Exam&lt;/u&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/th&gt;	&lt;th style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;u&gt;Bsc CSIT&lt;/u&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/th&gt;&lt;/tr&gt;  &lt;tr&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Exam Time&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;2 Hours&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Total marks&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;100 marks&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Pass marks&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;35 marks&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;   &lt;tr&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;b&gt;Physics&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;u&gt;25 marks&lt;/u&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;b&gt;Math&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
  &lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;u&gt;25 marks&lt;/u&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;b&gt;Chemistry&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;u&gt;25 marks&lt;/u&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;b&gt;English&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;u&gt;15 marks&lt;/u&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;b&gt;Computer G.K.&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;	&lt;td style=&quot;text-align: center;&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;&lt;u&gt;10 marks&lt;/u&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;hr /&gt;&lt;h2 style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;color: #0b5394; font-family: inherit; font-size: x-large;&quot;&gt;BSc CSIT entrance exam in Nepal&lt;/span&gt;&lt;/h2&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;BSc CSIT is a popular course in Nepal that combines the study of both computer science and information technology. It is offered at various colleges affiliated with Tribhuvan University, and is a four-year program divided into eight semesters. To be eligible for the BSc CSIT program, a student must have completed twelve years of schooling in the science stream or equivalent and must have passed the entrance examination administered by the Institute of Science and Technology (TU IOST), Tribhuvan University.&lt;/span&gt;&lt;/p&gt;&lt;/div&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;The entrance exam is held annually, (Around Kartik, Mangsir (&lt;a href=&quot;/2022/10/Bsc-Csit-Entrance-Model-questions-paper-tuiost-2079.html&quot;&gt;Click here for Exact date&lt;/a&gt;))&amp;nbsp; and the minimum required marks for admission vary from year to year, but the pass mark is 35 mark in total (acc. 2079).&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;Read: BSc CSIT Model Question&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: large;&quot;&gt;In addition to the &lt;a href=&quot;https://entrance.tuiost.edu.np/&quot; target=&quot;_blank&quot;&gt;entrance exam&lt;/a&gt;, students must also fulfill all of the application procedures and meet the minimum academic requirements for admission. Successful completion of the BSc CSIT program can lead to a range of career opportunities in the field of IT and computer science, both nationally and internationally.&lt;/span&gt;&lt;/p&gt;&lt;/div&gt;&lt;hr /&gt;&lt;h2 style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;color: #0b5394; font-family: inherit; font-size: x-large;&quot;&gt;Best BSc CSIT colleges in Nepal&lt;/span&gt;&lt;/h2&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;span style=&quot;font-family: inherit; font-size: l