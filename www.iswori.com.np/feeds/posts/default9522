&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;&lt;ins data-ad-client=&quot;ca-pub-9570381088286874&quot; data-ad-format=&quot;auto&quot; data-ad-slot=&quot;2837908956&quot; data-adsbygoogle-status=&quot;done&quot; data-full-width-responsive=&quot;true&quot;&gt;&lt;/ins&gt;

&lt;/span&gt;&lt;/span&gt;&lt;h4 id=&quot;essays&quot;&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;Essays&lt;/span&gt;&lt;/span&gt;&lt;/h4&gt;
&lt;figure&gt;&lt;table class=&quot;moz-reader-wide-table&quot;&gt;&lt;thead&gt;&lt;tr&gt;&lt;th&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;Chapter&lt;/span&gt;&lt;/span&gt;&lt;/th&gt;&lt;th&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;Title&lt;/span&gt;&lt;/span&gt;&lt;/th&gt;&lt;th&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;Link&lt;/span&gt;&lt;/span&gt;&lt;/th&gt;&lt;/tr&gt;&lt;/thead&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;1&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;On Libraries&amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;Exercise&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;2&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;Marriage as a Social Institution&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;Exercise&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;3&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;Knowledge and Wisdom&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;Exercise&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;span&gt;&lt;span style=&quot;font-family: verdana; font-size: large;&quot;&gt;4&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;